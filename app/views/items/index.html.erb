<div class="jumbotron text-center"><h1>Items</h1>
<p>Click an Item To View Description</p>
</div>


<div class = container>
<ul style="list-style:none;">
  <% @items.each do |item| %>
    <li><%= link_to item.name, department_item_path(@department, item) %></li>
  <% end %>
</ul>
<br/>
<button><%= link_to 'Create a new item', new_department_item_path(@department) %></button>
<br/>
<%= link_to 'Back To Department', department_path(@department) %></div>